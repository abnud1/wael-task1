{
    "compilerOptions": {
        "target": "esnext",
        "module": "esnext",
        "jsx": "react-jsx",
        "strict": true,
        "noEmit": true,
        "skipLibCheck": true,
        "moduleResolution": "node",
        "baseUrl": ".",
        "allowSyntheticDefaultImports":true,
        "paths": {
            "@util/*": ["./src/util/*"],
            "@store/*": ["./src/store/*"],
            "@store": ["./src/store"]
        }
    },
    "include": [
        "src"
    ]
}